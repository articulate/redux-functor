{
  "name": "redux-functor",
  "version": "0.0.0",
  "description": "Functor middleware for redux",
  "main": "index.js",
  "repository": "git@github.com:articulate/redux-functor.git",
  "author": "articulate",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
